type User {
  id: ID! @unique
  email: String @unique
  firstName: String!
  middleName: String
  lastName: String!
  password: String!
  createdAt: DateTime!
  updatedAt: DateTime!
  lastLoggedIn: DateTime
  role: Role! @default(value: Physician)
}

enum Role { 
  Admin
  Owner
  Physician
  Patient
  Provider
  Disabled
}


