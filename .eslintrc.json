{
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "eol-last": 0,
        "no-console": 0,
        "object-curly-newline": 0,
        "max-len": 0,
        "arrow-parens": ["error", "as-needed"],
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
        "no-underscore-dangle": 0,
        "no-return-assign": 0,
        "import/prefer-default-export": 0
    },
    "parser": "babel-eslint"
}